    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug app",
            "protocol": "inspector",
            "cwd": "${workspaceFolder}\\pwa",
            "program": "${workspaceFolder}\\pwa\\node_modules\\sapper\\dist\\cli.js",
            "args": [
                "dev"
            ],
            "console": "integratedTerminal",
            "autoAttachChildProcesses": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\pwa",
            "program": "${workspaceFolder}\\pwa\\node_modules\\sapper\\sapper",
            "args": [
                "dev"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dev server.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}\\pwa\\pwa",
            "program": "${workspaceFolder}\\pwa\\pwa\\__sapper__\\dev\\server\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug File",
            "program": "${file}"
        }
    ]
}